    "dataViewMappings": [
        {
            "conditions": [
                {
                    "timestamp": {
                        "max": 1
                    },
                    "series": {
                        "max": 1
                    },
                    "values": {
                        "max": 1
                    },
                    "filterField": {
                        "max": 1
                    },
                    "anomalies": {
                        "max": 1
                    },
                    "timelineMarkers": {
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "select": [
                        {
                            "for": {
                                "in": "timestamp"
                            }
                        },
                        {
                            "for": {
                                "in": "filterField"
                            }
                        }
                    ],
                    "dataReductionAlgorithm": {
                        "window": {
                            "count": 30000
                        }
                    }
                },
                "values": {
                    "group": {
                        "by": "series",
                        "select": [
                            {
                                "bind": {
                                    "to": "values"
                                }
                            },
                            {
                                "bind": {
                                    "to": "anomalies"
                                }
                            },
                            {
                                "bind": {
                                    "to": "timelineMarkers"
                                }
                            }
                        ],
                        "dataReductionAlgorithm": {
                            "top": {
                                "count": 100
                            }
                        }
                    }
                }
            }
        }
    ],
